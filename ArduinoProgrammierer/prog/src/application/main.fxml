<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <Button mnemonicParsing="false" onAction="#programClicked" text="Program" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="30.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <center>
      <GridPane hgap="30.0" vgap="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="SSIDTextFiield" />
            <TextField fx:id="PasswordTextField" GridPane.rowIndex="1" />
            <TextField fx:id="IDTextField" GridPane.rowIndex="2" />
            <Label text="SSID" GridPane.columnIndex="1" />
            <Label text="Password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="DeviceID" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <BorderPane.margin>
            <Insets bottom="20.0" left="50.0" right="50.0" top="50.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
</BorderPane>
